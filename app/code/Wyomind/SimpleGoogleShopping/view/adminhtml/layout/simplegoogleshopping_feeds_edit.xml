<?xml version="1.0"?><!-- Copyright Â© 2015 Wyomind. All rights reserved. See LICENSE.txt for license details.--><!-- Layout of the edit form of a google shopping data feed --><page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left"      xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">    <update handle="editor"/>    <!-- add css and js files -->    <head>        <css src="Wyomind_SimpleGoogleShopping::css/categories.css"/>        <css src="Wyomind_SimpleGoogleShopping::css/cron.css"/>        <css src="Wyomind_SimpleGoogleShopping::css/filters.css"/>        <css src="Wyomind_SimpleGoogleShopping::css/configuration.css"/>        <css src="Wyomind_SimpleGoogleShopping::css/blackbox.css"/>        <css src="Wyomind_Core::js/codemirror/codemirror.css"/>        <link src="Wyomind_Core::js/codemirror/codemirror.js"/>        <link src="Wyomind_Core::js/codemirror/formatting.js"/>        <link src="Wyomind_Core::js/codemirror/active-line.js"/>        <link src="Wyomind_Core::js/codemirror/matchbrackets.js"/>        <link src="Wyomind_Core::js/codemirror/htmlmixed/htmlmixed.js"/>        <link src="Wyomind_Core::js/codemirror/xml/xml.js"/>        <link src="Wyomind_Core::js/codemirror/javascript/javascript.js"/>        <link src="Wyomind_Core::js/codemirror/css/css.js"/>        <link src="Wyomind_Core::js/codemirror/clike/clike.js"/>        <link src="Wyomind_Core::js/codemirror/php/php.js"/>    </head>    <body>        <!-- translation -->        <referenceContainer name="page.js.translate">            <block class="Magento\Framework\View\Element\Template" name="js_translate" as="js_translate"                   template="Wyomind_SimpleGoogleShopping::edit/translate.phtml"/>        </referenceContainer>        <!-- main block -->        <referenceContainer name="content">            <block class="Wyomind\SimpleGoogleShopping\Block\Adminhtml\Feeds\Edit"                   name="simplegoogleshopping_feeds_edit"/>            <block class="Magento\Framework\View\Element\Template" name="simplegoogleshopping_feeds_edit_utils"                   template="Wyomind_SimpleGoogleShopping::edit/utils.js.phtml"/>            <block class="Magento\Framework\View\Element\Template" name="simplegoogleshoping_feeds_edit_blackbox"                   template="Wyomind_SimpleGoogleShopping::edit/blackbox.phtml"/>            <block class="Magento\Framework\View\Element\Template" name="simplegoogleshopping_feeds_edit_configuration"                   template="Wyomind_SimpleGoogleShopping::edit/configuration.phtml"/>        </referenceContainer>        <!-- left tabs -->        <referenceContainer name="left">            <block class="Wyomind\SimpleGoogleShopping\Block\Adminhtml\Feeds\Edit\Tabs"                   name="simplegoogleshopping_feeds_edit_tabs">                <!-- decalre blocks to use in the addTab method -->                <block class="Wyomind\SimpleGoogleShopping\Block\Adminhtml\Feeds\Edit\Tab\Configuration"                       name="simplegoogleshopping_feeds_edit_tab_configuration"/>                <block class="Wyomind\SimpleGoogleShopping\Block\Adminhtml\Feeds\Edit\Tab\Categories"                       template="edit/categories.phtml" name="simplegoogleshopping_feeds_edit_tab_categories"/>                <block class="Wyomind\SimpleGoogleShopping\Block\Adminhtml\Feeds\Edit\Tab\Filters"                       template="edit/filters.phtml" name="simplegoogleshopping_feeds_edit_tab_filters"/>                <block class="Wyomind\SimpleGoogleShopping\Block\Adminhtml\Feeds\Edit\Tab\Cron"                       template="edit/cron.phtml" name="simplegoogleshopping_feeds_edit_tab_cron"/>                <!-- add tabs -->                <action method="addTab">                    <argument name="name" xsi:type="string">configuration_section</argument>                    <argument name="block" xsi:type="string">simplegoogleshopping_feeds_edit_tab_configuration</argument>                </action>                <action method="addTab">                    <argument name="name" xsi:type="string">categories_section</argument>                    <argument name="block" xsi:type="string">simplegoogleshopping_feeds_edit_tab_categories</argument>                </action>                <action method="addTab">                    <argument name="name" xsi:type="string">filters_section</argument>                    <argument name="block" xsi:type="string">simplegoogleshopping_feeds_edit_tab_filters</argument>                </action>                <action method="addTab">                    <argument name="name" xsi:type="string">cron_section</argument>                    <argument name="block" xsi:type="string">simplegoogleshopping_feeds_edit_tab_cron</argument>                </action>            </block>        </referenceContainer>    </body></page>